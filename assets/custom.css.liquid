/* ====== Footer ===== */
@media screen and (max-width: 749px){
  .site-footer__content{
  	 padding: 0;
  }
  .site-footer__item{
  	padding: 0;
    margin-bottom: 30px;
    width: 100%;
    margin-left: 0;
    margin-right: 0;
  }
  .site-footer .site-footer-expand .site-footer__title.h4{
    font-size: 16px;
    line-height: 22px;  
    font-weight: 500;
    position: relative;
  }
  .site-footer__linklist-item a{
    font-size: 14px;
    line-height: 16px;   	
    font-weight: 400;
  }
  .site-footer-expand{
  	position: relative;
  }
  .site-footer__item-inner{
  	width: 100%;
    overflow: hidden;
  }
  .site-footer-expand .site-footer__title.h4:before{
	position: absolute;
    width: 6px;
    height: 2px;
    right: 6px;
    top: 14px;
    content: ' ';
    background: #3B3A40;
    transform: rotate(-45deg);
  }
  .site-footer-expand .site-footer__title.h4:after{
	position: absolute;
    width: 6px;
    height: 2px;
    right: 2px;
    top: 14px;
    content: ' ';
    background: #3B3A40;
    transform: rotate(-135deg);
  }
  .site-footer-expand.expanded .site-footer__title.h4:before{
  	transform: rotate(45deg);
  }
  .site-footer-expand.expanded .site-footer__title.h4:after{
  	transform: rotate(135deg);
  }
  .site-footer-expand .site-footer__linklist{
  	height: 0;
  }
  .site-footer-expand.expanded .site-footer__linklist{
  	height: auto;
  }
  .site-footer__bottomnav li a{
    font-size: 12px;
    line-height: 16px;   	
    font-weight: 400;  	
  }
  .site-footer__bottomnav li{
  	padding-right: 16px;
  }
}
